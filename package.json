{
  "name": "gmail-inbox-manager",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@netlify/functions": "^4.2.1",
    "@supabase/supabase-js": "^2.86.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "googleapis": "^154.1.0",
    "lucide-react": "^0.535.0",
    "netlify-lambda": "^2.0.16",
    "node-cron": "^4.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "concurrently \"netlify functions:serve --port 8888\" \"react-scripts start\" --names \"functions,react\" --prefix-colors \"blue,green\"",
